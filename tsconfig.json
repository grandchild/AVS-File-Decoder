{
    "compileOnSave": true,
    "compilerOptions": {
			"declaration": true,
			"esModuleInterop": true,
			"module": "esnext",
			"moduleResolution": "node",
			"noImplicitAny": false,
			"lib": [
				"es2020",
				"dom"
			],
			"outDir": "./dist",
			"removeComments": false,
			"rootDir": "src",
			"sourceMap": true,
			"target": "es6"
    },
    "exclude": [
        ".vscode-test",
        "node_modules"
    ]
}

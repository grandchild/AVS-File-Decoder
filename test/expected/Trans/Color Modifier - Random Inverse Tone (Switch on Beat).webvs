{"name":"Color Modifier - Random Inverse Tone (Switch on Beat)","clearFrame":true,"components":[{"type":"ColorModifier","group":"Trans","code":{"init":"","perFrame":"","onBeat":"token = rand(99) % 3;\r\ndr = if (equal(token, 0), -1, 1);\r\ndg = if (equal(token, 1), -1, 1);\r\ndb = if (equal(token, 2), -1, 1);","perPoint":"dd = red * 1.5;\r\nred = pow(dd, dr);\r\ngreen = pow(dd, dg);\r\nblue = pow(dd, db);"},"recomputeEveryFrame":true}]}
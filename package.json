{
  "name": "@visbot/webvsc",
  "version": "3.0.0-alpha.2",
  "description": "A library to convert Winamp AVS presets into Webvs JSON",
  "license": "MIT",
  "exports": {
    "import": "./dist/webvsc.js"
  },
  "type": "module",
  "typings": "./dist",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "directories": {
    "doc": "doc",
    "tests": "tests"
  },
  "contributors": [
    "grandchild",
    "idleberg"
  ],
  "homepage": "https://github.com/grandchild/AVS-File-Decoder#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/grandchild/AVS-File-Decoder.git"
  },
  "bugs": {
    "url": "https://github.com/grandchild/AVS-File-Decoder/issues"
  },
  "keywords": [
    "avs",
    "webvs",
    "webvsc",
    "cli",
    "converter",
    "winamp"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf ./dist",
    "dev": "npm run start",
    "lint:eslint": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "prepack": "npm run build",
    "prepare": "husky install",
    "start": "npm run build -- --watch",
    "test": "uvu tests -r tsm"
  },
  "dependencies": {
    "log-symbols": "^6.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^18.18.9",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "globby": "^14.0.0",
    "husky": "^8.0.3",
    "knip": "^2.41.3",
    "lint-staged": "^15.1.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^2.8.1",
    "rimraf": "^5.0.5",
    "tsm": "^2.3.0",
    "tsup": "^7.3.0",
    "typescript": "^5.2.2",
    "uvu": "^0.5.6"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}

{
  "name": "@visbot/webvsc",
  "version": "2.3.0",
  "description": "A library to convert Winamp AVS presets into Webvs JSON",
  "license": "MIT",
  "main": "./dist/node.cjs",
  "module": "./dist/node.mjs",
  "exports": {
    "./browser": {
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.cjs"
    },
    "./node": {
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs"
    }
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "contributors": [
    "grandchild",
    "idleberg"
  ],
  "homepage": "https://github.com/grandchild/AVS-File-Decoder#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/grandchild/AVS-File-Decoder.git"
  },
  "bugs": {
    "url": "https://github.com/grandchild/AVS-File-Decoder/issues"
  },
  "keywords": [
    "avs",
    "webvs",
    "webvsc",
    "cli",
    "converter",
    "winamp"
  ],
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "build:browser": "esbuild src/browser.ts --bundle --outfile=dist/node.mjs --format=esm",
    "build:node": "esbuild src/node.ts --bundle --outfile=dist/node.mjs --platform=node --format=esm",
    "build": "npm-run-all --parallel build:*",
    "clean": "rimraf ./dist",
    "dev": "npm run start",
    "lint:code": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "prepack": "npm run build",
    "prepare": "husky install",
    "start:browser": "esbuild src/browser.ts --bundle --outfile=dist/node.mjs --format=esm --watch",
    "start:node": "esbuild src/node.ts --bundle --outfile=dist/node.mjs --platform=node --format=esm --watch",
    "start": "npm-run-all --parallel start:*",
    "test": "uvu test"
  },
  "dependencies": {
    "log-symbols": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "esbuild": "^0.15.10",
    "eslint": "^8.24.0",
    "globby": "^13.1.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all2": "^6.0.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.0",
    "typescript": "^4.8.4",
    "uvu": "^0.5.6"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "volta": {
    "node": "18.10.0",
    "npm": "8.19.2",
    "yarn": "1.22.11"
  }
}
